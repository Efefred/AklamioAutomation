{
  "name": "aklamioautomation",
  "version": "1.0.0",
  "description": "This is a test automation framework for Aklamio",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "npx cypress open",
    "run:allure": "npx cypress run --env allure=true",
    "run:allure:report": "allure generate allure-results --clean -o allure-report",
    "run:chrome:headed": "cypress run --headed --browser chrome",
    "run:firefox:headed": "cypress run --headed --browser firefox",
    "run:edge:headed": "cypress run --headed --browser edge",
    "run:chrome": "cypress run --browser chrome",
    "run:firefox": "cypress run --browser firefox",
    "run:edge": "cypress run --browser edge",
    "run:twobrowsers": "npm run run:chrome && npm run run:firefox"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.24.0",
    "cypress-cucumber-preprocessor": "^4.3.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "dependencies": {
    "cypress": "^9.4.1"
  }
}
